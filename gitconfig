[alias]
  log1 = log --oneline
	cl = config --list
	cmp = difftool
	co = checkout
	com = commit -m
	diffnc = diff --no-color
  logauthor = log --pretty=format:'%h %ad | %s%d [%an]'--graph --date=short
  logsummary = log --pretty=format:'%h %ad | %s%d' --graph --date=short
	lol = log --graph --decorate --oneline --all
	oops = rm --cached
	pom = push -u origin master
	rfetch = fetch origin
	rpush = push -u origin master
	rpull = pull origin master
	rshow = remote -v show
	addurl = remote add origin
	rseturl = remote set-url origin
	stat = status
	wtf = whatchanged
